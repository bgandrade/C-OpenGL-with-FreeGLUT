{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build (make)",
      "type": "shell",
      "command": "mingw32-make",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "clean (make)",
      "type": "shell",
      "command": "mingw32-make clean",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": []
    }
  ]
}
